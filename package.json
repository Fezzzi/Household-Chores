  "scripts": {
    "start": "node code/server/dist/bundle.js",
    "dev": "concurrently --kill-others-on-fail -n web,api \"npm:start-web\" \"npm:start-api\"",
    "start-web": "webpack-dev-server --config config/webpack-front.config.js --open --mode development",
    "start-api": "webpack --watch --config config/webpack-back.config.js --mode development",
    "build": "npm run build-web && npm run build-api",
    "build-web": "rimraf dist/ && webpack --config config/webpack-front.config.js --mode production",
    "build-api": "rimraf code/server/dist && webpack --config config/webpack-back.config.js --mode production",
    "test": "npm run lint && npm run mocha",
    "lint": "eslint -c config/eslintrc.js . --ext .js,.jsx,.ts",
    "lint-fix": "eslint --fix -c config/eslintrc.js . --ext .js,.jsx,.ts",
    "mocha": "mocha -r ts-node/register -r tsconfig-paths/register -r config/mocha-env-config.js \"code/tests/**/*.spec.ts\"",
    "migrate": "ts-node -r tsconfig-paths/register node_modules/node-pg-migrate/bin/node-pg-migrate -m code/server/migrations --template-file-name config/migration-template.ts",
    "clean-uploads": "ts-node -r tsconfig-paths/register bin/clean-uploads.js",
    "clean-activity-table": "ts-node -r tsconfig-paths/register bin/clean-activity-table.js",
    "configure": "cp config/git-hooks/pre-push .git/hooks/pre-push"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
