  "scripts": {
    "start": "node code/server/dist/bundle.js",
    "dev": "start npm run start-front && start npm run start-back",
    "start-front": "webpack-dev-server --config config/webpack-front.config.js --open --mode development",
    "start-back": "webpack --watch --config config/webpack-back.config.js --mode development",
    "build": "npm run build-front && npm run build-back",
    "build-front": "webpack --config config/webpack-front.config.js --mode production",
    "build-back": "webpack --config config/webpack-back.config.js --mode production",
    "test": "npm run lint && npm run mocha",
    "lint": "eslint --cache -c config/eslintrc.js . --ext .js,.jsx,.ts",
    "lint-fix": "eslint --fix --cache -c config/eslintrc.js . --ext .js,.jsx,.ts",
    "mocha": "mocha -r ts-node/register -r tsconfig-paths/register -r ./config/mocha-env-config.js \"code/tests/**/*.spec.ts\"",
    "migrate": "ts-node -r tsconfig-paths/register config/migration.js",
    "clean-uploads": "ts-node -r tsconfig-paths/register bin/clean-uploads.js",
    "clean-activity-table": "ts-node -r tsconfig-paths/register bin/clean-activity-table.js",
    "configure": "cp config/git-hooks/pre-push .git/hooks/pre-push"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
