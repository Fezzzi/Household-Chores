  "scripts": {
    "start": "npm run start-back",
    "start-front": "webpack-dev-server --config config/webpack.config.js --open --mode development",
    "start-back": "ts-node -r tsconfig-paths/register code/server/src",
    "build": "webpack --config config/webpack.config.js --mode production",
    "test": "npm run lint && npm run mocha",
    "lint": "eslint --cache -c ./config/eslintrc.js . --ext .js,.jsx,.ts",
    "lint-fix": "eslint --fix --cache -c ./config/eslintrc.js . --ext .js,.jsx,.ts",
    "mocha": "cross-env-file -p ./config/mocha-env.json mocha -r ts-node/register -r tsconfig-paths/register \"tests/**/*.spec.ts\"",
    "migrate": "ts-node -r tsconfig-paths/register config/migration.js",
    "configure": "cp ./config/git-hooks/pre-push ./.git/hooks/pre-push"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
